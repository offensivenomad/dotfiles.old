{
   "applications": {
      "gecko": {
         "id": "pericles@alexandrucalin.me"
      }
   },
   "background": {
      "scripts": [ "background-vendors.js", "background-bundle.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icon/128-on.png",
         "16": "icon/16-on.png",
         "32": "icon/32-on.png",
         "96": "icon/96-on.png"
      },
      "default_popup": "popup.html",
      "default_title": "Pericles: Text to speech Screen Reader"
   },
   "content_scripts": [ {
      "css": [  ],
      "js": [ "content-vendors.js", "content-bundle.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "Web-page TTS (text to speech) screen reader, that does not require cutting and pasting sentences or entire page sections",
   "homepage_url": "http://pericles.alexandrucalin.me",
   "icons": {
      "128": "icon/128-on.png",
      "16": "icon/16-on.png",
      "32": "icon/32-on.png",
      "96": "icon/96-on.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Pericles: Text to Speech Screen Reader",
   "permissions": [ "activeTab", "storage", "contextMenus", "tts", "ttsEngine" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.7"
}
