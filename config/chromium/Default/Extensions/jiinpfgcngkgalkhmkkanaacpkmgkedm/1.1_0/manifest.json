{
   "background": {
      "scripts": [ "keycodes.js", "tabs.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "SpeakSel19.png",
      "default_title": "Speak Selection"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "keycodes.js", "content_script.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Speaks the current selection out loud.",
   "icons": {
      "128": "SpeakSel128.png",
      "16": "SpeakSel16.png",
      "256": "SpeakSel256.png",
      "48": "SpeakSel48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "14",
   "name": "Speak Selection",
   "options_page": "options.html",
   "permissions": [ "\u003Call_urls>", "tts", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1"
}
