   "externally_connectable": {
      "matches": [ "*://*.naturalreaders.com/*" ]
   },
   "icons": {
      "128": "assets/img/58@3x.png",
      "32": "assets/img/natreader.png",
      "48": "assets/img/58@2x.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Natural Reader Text to Speech",
   "permissions": [ "\u003Call_urls>", "activeTab", "contextMenus", "background", "storage", "tts", "webNavigation" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.1",
   "web_accessible_resources": [ "assets/*", "injected/*", "download/*" ]
}
