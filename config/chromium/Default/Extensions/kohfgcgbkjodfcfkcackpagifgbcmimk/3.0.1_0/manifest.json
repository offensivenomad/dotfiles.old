{
   "background": {
      "persistent": true,
      "scripts": [ "background/const.js", "background/chromeRuntimeMessage.js", "background/amplify-auth.js", "background/amplify-auth-config.js", "background/storage.js", "background/pdf.js", "background/script-injector.js", "background/utils.js", "background/voices.js", "background/widget.js", "background/extension.js", "background/alert-handler.js", "background/tts-text.js", "background/tts.js", "background/reader.js", "background/auth.js", "libs/compromise.min.js", "background/context.js", "background/convert.js", "background/prorate.js" ]
   },
   "browser_action": {

   },
   "commands": {
      "forward": {
         "description": "Next",
         "suggested_key": {
            "default": "Alt+Down"
         }
      },
      "play": {
         "description": "Read/Pause",
         "suggested_key": {
            "default": "Alt+R"
         }
      },
      "rewind": {
         "description": "Previous",
         "suggested_key": {
            "default": "Alt+Up"
         }
      },
      "speaker": {
         "description": "Speaker",
         "suggested_key": {
            "default": "Alt+S"
         }
      },
      "toggleShowReadIcon": {
         "description": "Toggle Show Read Selection Icon"
      }
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
